#include<bits/stdc++.h>


using namespace std;
int main () {

int k;
while(true){
    cin>>k;

        if(k==0) break;
 fflush(stdin);
        int a,b;
        cin>>a>>b;
        for(int i=1;i<=k;i++){
            fflush(stdin);
            //if(k==0)break;
    int x,y;
    cin>>x>>y;
    if(x==a || y==b){
        cout<<"divisa"<<endl;
    }
    if(x>a && y>b){
        cout<<"NE"<<endl;

    }
    if(x<a && y >b){
        cout<<"NO"<<endl;
    }
    if(x<a && y<b){
        cout<<"SO"<<endl;
    }
    if(x>a && y<b){
        cout<<"SE"<<endl;
    }
        }
}


}
